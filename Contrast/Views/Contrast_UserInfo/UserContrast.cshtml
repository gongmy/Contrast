@{
    ViewBag.Title = "与用户匹配的组织信息";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@model List<Entity.User_Organization>
<div>
    <table class="Tab_list" cellpadding="0" cellspacing="0">
        <tr>
            <th>
                公司名称
            </th>
            <th>
                需求资金(万元)
            </th>
            <th>
                需求周期(月)
            </th>
            <th>
                可承受年利息
            </th>
            <th>
                组织机构
            </th>
            <th>
                提供资金(万元)
            </th>
            <th>
                提供周期(月)
            </th>
            <th>
                年利息
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.user.CompanyName
                </td>
                <td>
                    @item.user.DemandMoney
                </td>
                <td>
                    @item.user.DemandMonth
                </td>
                <td>
                    @item.user.AcceptInterest%
                </td>
                <td>
                    @item.org.Name
                </td>
                <td>
                    @item.org.ProvideMoney
                </td>
                <td>
                    @item.org.BeginMonth - @item.org.EndMonth
                </td>
                <td>
                    @item.org.DemandInterest%
                </td>
            </tr>
        }
    </table>
</div>
