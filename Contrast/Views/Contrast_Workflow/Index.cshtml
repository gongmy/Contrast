@{
    ViewBag.Title = "审批配置";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@model List<Entity.Contrast_Workflow>

<div>
    <table class="Tab_list" cellpadding="0" cellspacing="0">
        <tr>
            <th>
                步骤
            </th>
            <th>
                标题
            </th>
            <th>
                审批人
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Sort
                </td>
                <td>
                    @item.Title
                </td>
                <td>
                    @(item.IsSelfCheck==false?item.Contrast_Account.Name:"负责人审批")
                </td>
            </tr>
        }
    </table>
</div>
